 = ref(tasksdata);\n      function addtodo() {\n        if (newtodo.value) {\n          tasks.value.push({\n            done : false,\n            content : newtodo.value\n          });\n\n          newtodo.value = \"\";\n          savedata();\n        }\n        function savedata() {\n          const storagedata = JSON.stringify(tasks.value);\n          localStorage.setItem(\"tasks\", storagedata);\n        }\n        return {\n          tasks,\n          newtodo,\n          addtodo,\n          savedata\n        }\n      }\n    }\n  }\n</script>"],"mappings":";AAyBE,SAASA,GAAE,QAAS,KAAI;AACxB,eAAe;EACbC,IAAG,EAAI,KAAK;EACZC,KAAKA,CAAA,EAAG;IACN,MAAMC,OAAM,GAAIH,GAAG,CAAC,EAAE,CAAC;IACvB,MAAMI,WAAU,GAAI,CAAC;MACnBC,IAAG,EAAI,IAAI;MACXC,OAAM,EAAI;IACZ,CAAC,CAAC;IACF,MAAMC,SAAQ,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,KAAIP,WAAW;IACzE,MAAMQ,KAAI,GAAIZ,GAAG,CAACO,SAAS,CAAC;IAC5B,SAASM,OAAOA,CAAA,EAAG;MACjB,IAAIV,OAAO,CAACW,KAAK,EAAE;QACjBF,KAAK,CAACE,KAAK,CAACC,IAAI,CAAC;UACfV,IAAG,EAAI,KAAK;UACZC,OAAM,EAAIH,OAAO,CAACW;QACpB,CAAC,CAAC;QAEFX,OAAO,CAACW,KAAI,GAAI,EAAE;QAClBE,QAAQ,CAAC,CAAC;MACZ;MACA,SAASA,QAAQA,CAAA,EAAG;QAClB,MAAMC,WAAU,GAAIT,IAAI,CAACU,SAAS,CAACN,KAAK,CAACE,KAAK,CAAC;QAC/CJ,YAAY,CAACS,OAAO,CAAC,OAAO,EAAEF,WAAW,CAAC;MAC5C;MACA,OAAO;QACLL,KAAK;QACLT,OAAO;QACPU,OAAO;QACPG;MACF;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}